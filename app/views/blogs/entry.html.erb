<% add_to_page_title @entry.feature.present? ? "#{@entry.feature.name}: #{@entry.headline}" : @entry.headline %>
<%= render_content @entry, "opengraph" %>
<%= render 'shared/ads/dfp_script_config', category: @entry.category %>
<% content_for :membership_copy, @category.present? ? @category.title : 'Lorem Ipsum' %>
<% content_for :body_class, 'interior single' %>

<%= cache ["show", "v12", @entry] do %>
  <%= render "shared/new/single_blog", article: @entry, full: true %>
<% end %>

