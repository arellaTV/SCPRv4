<% add_to_page_title @story.headline %>
<%= render_content @story, "opengraph" %>
<%= render 'shared/ads/dfp_script_config', category: @story.category %>

<%= cache ["show", "v11", @story] do %>
  <%= render "news/story", story: @story %>
<% end %>

<div class="fold inline" id="fold">
  <%= render "shared/related_content_and_links", content: @story %>
  <%= content_widget 'content_carousel', @story %>
  <%= comments_for @story %>
</div>

<%= render 'shared/scripts/embedder' %>
