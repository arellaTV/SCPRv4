<table class="table table-striped problems <%= problem_type %>">
  <thead>
    <tr>
      <td></td>
      <td>Ends At</td>
      <td></td>
      <td>Starts At</td>
    </tr>
  </thead>
  <tbody>
    <% problems.each do |problem| %>
    <tr>
      <td><strong><%= problem[0].metadata.display_name %></strong></td>
      <td><%= problem[0].display_ends_at %></td>
      <td><strong><%= problem[1].metadata.display_name %></strong></td>
      <td><%= problem[1].display_starts_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>